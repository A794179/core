{
  "problemMatcher": [
    {
      "owner": "pylint",
      "pattern": [
        {
          "regexp": "^((\\/github\\/workspace\\/)?.+):(\\d+):(\\d+):\\s([CERW]\\d{4}):\\s(.+)$",
          "file": 1,
          "line": 3,
          "column": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
